{
  "name": "@truffle-ai/saiki",
  "version": "0.2.1",
  "description": "Your command center for controlling computers and services with natural language - connect once, command everything",
  "type": "module",
  "private": true,
  "main": "dist/app/index.js",
  "bin": {
    "saiki": "./dist/app/index.js"
  },
  "scripts": {
    "clean": "pnpm -r --parallel clean",
    "prebuild": "",
    "build": "pnpm -r build",
    "prepare": "husky install",
    "copy-client": "npx rimraf public && mkdir public && npx copyfiles -f \"app/web/client/*\" public",
    "start": "node dist/app/index.js",
    "dev": "tsc --watch",
    "lint": "pnpm -r --parallel lint",
    "lint:fix": "pnpm -r --parallel lint:fix",
    "format": "prettier --write \"packages/**/*.ts\"",
    "format:check": "prettier --check \"packages/**/*.ts\"",
    "test": "pnpm -r test",
    "test:ci": "pnpm -r test:ci",
    "test:watch": "pnpm -r test:watch",
    "list": "node scripts/dev.js list",
    "generate": "node scripts/dev.js generate",
    "parse-prd": "node scripts/dev.js parse-prd"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "cli",
    "natural-language",
    "openai",
    "truffle-ai"
  ],
  "author": "",
  "license": "Elastic-2.0",
  "files": [
    "dist/",
    "configuration/saiki.yml",
    "public/"
  ],
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.2",
    "@ai-sdk/google": "^1.2.8",
    "@ai-sdk/openai": "^1.3.3",
    "@anthropic-ai/sdk": "^0.39.0",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "ai": "^4.2.6",
    "boxen": "^7.1.1",
    "chalk": "^5.4.1",
    "commander": "^11.1.0",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.19.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "marked": "^15.0.8",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.89.0",
    "ora": "^7.0.1",
    "puppeteer-core": "^24.4.0",
    "readline-sync": "^1.4.10",
    "rimraf": "^6.0.1",
    "tiktoken": "^1.0.21",
    "typescript": "^5.3.3",
    "winston": "^3.17.0",
    "ws": "^8.18.1",
    "yaml": "^2.7.1",
    "zod": "^3.24.3",
    "cli-table3": "^0.6.5",
    "cors": "^2.8.5",
    "fastmcp": "^1.20.5",
    "figlet": "^1.8.0",
    "fuse.js": "^7.0.0",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^12.5.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.36",
    "@types/readline-sync": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "vitest": "^3.1.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}