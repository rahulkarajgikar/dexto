{
    "name": "dexto-monorepo",
    "version": "1.1.2",
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=8.3.0"
    },
    "description": "Your command center for controlling computers and services with natural language - connect once, command everything",
    "type": "module",
    "private": true,
    "packageManager": "pnpm@10.12.4",
    "scripts": {
        "clean": "rimraf dist build public coverage logs .eslintcache test-temp && pnpm run clean:storage && tsx scripts/clean-build-files.ts",
        "clean:storage": "rimraf .dexto",
        "prebuild": "",
        "build": "pnpm run clean && pnpm run build:all && pnpm run copy-webui-dist",
        "build:core": "pnpm -C src/packages/core build",
        "build:webui": "BUILD_STANDALONE=true pnpm -C src/packages/webui build",
        "build:webui:dev": "pnpm -C src/packages/webui build",
        "build:cli": "pnpm -C src/packages/cli build",
        "build:cli-only": "pnpm run build:core && pnpm run build:cli",
        "build:all": "pnpm run build:core && pnpm run build:webui && pnpm run build:cli && pnpm run embed-webui",
        "embed-webui": "tsx scripts/copy-webui-dist.ts",
        "link-cli": "pnpm run build:all && npm uninstall -g dexto 2>/dev/null || true && cd src/packages/cli && pnpm link --global",
        "link-cli-fast": "pnpm run build:cli-only && npm uninstall -g dexto 2>/dev/null || true && cd src/packages/cli && pnpm link --global",
        "unlink-cli": "pnpm rm dexto --global 2>/dev/null || true && npm uninstall -g dexto 2>/dev/null || true",
        "install-cli": "pnpm run build:all && tsx scripts/install-global-cli.ts",
        "install-cli-fast": "pnpm run build:cli-only && tsx scripts/install-global-cli.ts",
        "dev:cli": "pnpm run link-cli",
        "prepack": "pnpm run build",
        "build:check": "pnpm run typecheck && pnpm run build",
        "build:strict": "pnpm run typecheck && pnpm run build",
        "typecheck": "turbo run typecheck",
        "typecheck:core": "tsc --noEmit src/packages/core/src/**/*.ts",
        "typecheck:watch": "turbo run typecheck --watch",
        "prepare": "husky",
        "build-webui": "npx rimraf src/packages/webui/node_modules src/packages/webui/package-lock.json && pnpm -C src/packages/webui install && pnpm -C src/packages/webui build",
        "copy-webui-dist": "npx tsx scripts/copy-webui-dist.ts",
        "start": "pnpm -C src/packages/cli start",
        "dev": "tsc --watch",
        "lint": "turbo run lint",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\"",
        "test": "vitest run",
        "test:unit": "vitest run --exclude '**/*.integration.test.ts'",
        "test:integ": "vitest run --config vitest.integration.config.ts",
        "test:ci": "vitest run --coverage",
        "test:watch": "vitest watch",
        "test:unit:watch": "vitest watch --exclude '**/*.integration.test.ts'",
        "test:integ:watch": "vitest watch --config vitest.integration.config.ts",
        "repo:build": "turbo run build",
        "repo:test": "turbo run test",
        "repo:lint": "turbo run lint",
        "repo:typecheck": "turbo run typecheck"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "ai",
        "cli",
        "natural-language",
        "openai",
        "truffle-ai",
        "dexto"
    ],
    "author": "",
    "license": "Elastic-2.0",
    "files": [
        "dist/",
        "agents/agent-registry.json",
        "agents/agent-template.yml",
        "agents/default-agent.yml",
        "agents/database-agent/",
        "agents/talk2pdf-agent/",
        "agents/image-editor-agent/",
        "agents/music-agent/",
        "agents/product-name-researcher/",
        "agents/triage-demo/",
        "public/"
    ],
    "dependencies": {
        "@ai-sdk/anthropic": "^2.0.0",
        "@ai-sdk/cohere": "^2.0.0",
        "@ai-sdk/google": "^2.0.0",
        "@ai-sdk/groq": "^2.0.0",
        "@ai-sdk/openai": "^2.0.0",
        "@ai-sdk/xai": "^2.0.0",
        "@anthropic-ai/sdk": "^0.60.0",
        "@clack/prompts": "^0.10.1",
        "@modelcontextprotocol/sdk": "^1.17.2",
        "ai": "^5.0.0",
        "better-sqlite3": "^11.10.0",
        "boxen": "^7.1.1",
        "chalk": "^5.4.1",
        "commander": "^11.1.0",
        "discord.js": "^14.19.3",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "fs-extra": "^11.3.0",
        "grammy": "^1.36.3",
        "ioredis": "^5.6.1",
        "marked": "^15.0.8",
        "openai": "^5.16.0",
        "ora": "^7.0.1",
        "pg": "^8.16.0",
        "readline-sync": "^1.4.10",
        "rimraf": "^6.0.1",
        "tiktoken": "^1.0.21",
        "typescript": "^5.3.3",
        "winston": "^3.17.0",
        "ws": "^8.18.1",
        "yaml": "^2.7.1",
        "zod": "^3.25.0",
        "zod-to-json-schema": "^3.24.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.23.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/express": "^4.17.21",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.17.36",
        "@types/pg": "^8.15.4",
        "@types/readline-sync": "^1.4.8",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "@vitest/coverage-v8": "^3.1.3",
        "cross-env": "^7.0.3",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.1",
        "prettier": "^3.5.3",
        "supertest": "^7.1.0",
        "ts-node": "^10.9.2",
        "tsup": "^8.4.0",
        "tsx": "^4.19.2",
        "turbo": "^2.0.14",
        "vitest": "^3.1.3"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write"
        ]
    },
    "overrides": {
        "tough-cookie": "^4.1.3"
    },
    "preinstall": "npx only-allow pnpm"
}
